function obterBairros(urlBase, origem, div){
	
	var idsCidade = document.getElementsByName('cidadesSelecionadas');
	var valoresCidades = "";
	for(var i = 0; i < idsCidade.length; i++){
        valoresCidades += "&cidadesSelecionadas=" + idsCidade[i].value;
